{
  "name": "fcp-sfd-object-processor",
  "version": "0.2.0",
  "description": "Messaging gateway service for Single Front Door",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "pretest": "npm run test:lint",
    "docker:debug": "docker compose -f compose.yaml -f compose.debug.yaml -p 'fcp-sfd-object-processor' up",
    "docker:dev": "docker compose up",
    "docker:test": "npm run test:lint && mkdir -p coverage 2>/dev/null || true docker compose -p fcp-sfd-object-processor-test down -v && docker compose -f compose.yaml -f compose.test.yaml -p fcp-sfd-object-processor-test run --build --rm fcp-sfd-object-processor",
    "docker:test:watch": "docker compose -p fcp-sfd-object-processor-test down -v && docker compose -f compose.yaml -f compose.test.yaml -f compose.test.watch.yaml -p fcp-sfd-object-processor-test run --build --rm fcp-sfd-object-processor",
    "test": "rm -rf ./coverage/** && vitest run --coverage",
    "test:watch": "vitest watch",
    "test:lint": "npx eslint",
    "start:watch": "nodemon --watch src --exec 'node --experimental-vm-modules --inspect=0.0.0.0 src/index.js'",
    "start:debug": "nodemon --watch src --exec 'node --experimental-vm-modules --inspect-brk=0.0.0.0 src/index.js'",
    "start": "node --experimental-vm-modules src/index.js"
  },
  "author": "Defra DDTS",
  "contributors": [
    "Amirs Ishmuhametovs amirs.ishmuhametovs@defra.gov.uk>",
    "Simon Dunn <simon.dunn1@defra.gov.uk>"
  ],
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-tracing": "1.0.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.3.12",
    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "applicationinsights": "3.7.0",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "global-agent": "3.0.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "hapi-swagger": "17.3.2",
    "http-status-codes": "2.3.0",
    "https-proxy-agent": "7.0.5",
    "joi": "17.13.3",
    "mongo-locks": "3.0.2",
    "mongodb": "6.10.0",
    "node-fetch": "3.3.2",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "undici": "6.21.3"
  },
  "devDependencies": {
    "@types/convict": "6.1.6",
    "@types/node": "22.13.4",
    "@types/webpack-assets-manifest": "5.1.4",
    "@vitest/coverage-v8": "3.1.3",
    "autoprefixer": "10.4.20",
    "eslint": "9.29.0",
    "neostandard": "0.12.2",
    "nodemon": "3.1.7",
    "npm-run-all": "4.1.5",
    "vitest": "3.1.3"
  }
}
